  object qry: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO Stuff'
      '  (Name, Code, StuffCategoryID, Activated)'
      'VALUES'
      '  (:Name, :Code, :StuffCategoryID, :Activated)'
      'SET :ID = SCOPE_IDENTITY()')
    SQLDelete.Strings = (
      'DELETE FROM Stuff'
      'WHERE'
      '  ID = :Old_ID')
    SQLUpdate.Strings = (
      'UPDATE Stuff'
      'SET'
      
        '  Name = :Name, Code = :Code, StuffCategoryID = :StuffCategoryID' +
        ', Activated = :Activated'
      'WHERE'
      '  ID = :Old_ID')
    SQLRefresh.Strings = (
      'SELECT Name, Code, UnitID, StuffCategoryID, Activated FROM Stuff'
      'WHERE'
      '  ID = :ID')
    SQLRecCount.Strings = (
      'SET :PCOUNT = (SELECT COUNT(*) FROM Stuff'
      ')')
    Connection = dmPakhsh.Con
    SQL.Strings = (
      'SELECT s.ID,'
      '       s.Name AS StuffName,'
      '       s.Code,'
      '       s.StuffCategoryID,'
      '       ac.Name AS StuffCategoryName,'
      '       s.Activated'
      'FROM Stuff AS s'
      'LEFT JOIN StuffCategory AS ac ON(s.StuffCategoryID = ac.ID)'
      'ORDER By s.Name , s.Activated')
