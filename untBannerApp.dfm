  object qry: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO app.Banner'
      '  (Activated, StuffBrandID, FromDate, ToDate)'
      'VALUES'
      '  (:Activated, :StuffBrandID, :FromDate, :ToDate)'
      'SET :ID = SCOPE_IDENTITY()')
    SQLDelete.Strings = (
      'DELETE FROM app.Banner'
      'WHERE'
      '  ID = :Old_ID')
    SQLUpdate.Strings = (
      'UPDATE app.Banner'
      'SET'
      
        '  Activated = :Activated, StuffBrandID = :StuffBrandID, FromDate' +
        ' = :FromDate, ToDate = :ToDate'
      'WHERE'
      '  ID = :Old_ID')
    SQLRefresh.Strings = (
      'SELECT Activated, StuffBrandID, FromDate, ToDate FROM app.Banner'
      'WHERE'
      '  ID = :ID')
    Connection = dmPakhsh.Con
    SQL.Strings = (
      'SELECT  b.ID,'
      '        b.Activated,'
      '        b.StuffBrandID,'
      #9's.Name + '#39' '#39' + d.Name AS StuffBrandName,'
      '        b.FromDate,'
      '        b.ToDate'
      'FROM app.Banner as b'
      'LEFT JOIN StuffBrand AS sb ON(b.stuffBrandID = sb.ID)'
      'LEFT JOIN Stuff as s ON(sb.StuffID = s.ID)'
      'LEFT JOIN Brand as d ON(sb.BrandID = d.ID)')
