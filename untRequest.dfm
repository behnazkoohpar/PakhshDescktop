  object qryDetailes: TUniQuery
    SQLUpdate.Strings = (
      ''
      '')
    Connection = dmPakhsh.Con
    SQL.Strings = (
      'SELECT ri.ID,'
      '      ri.RequestID,'
      '      ri.StuffBrandID,'
      '      ri.Count,'
      '      sb.Code AS StuffBrandCode,'
      '      s.Name AS StuffName,'
      '      b.Name AS BrandName,'
      '      s.Name + '#39' '#39' + b.Name AS StuffBrandName,'
      '      sbp.Price, '
      '      sbp.Offer,'
      
        '      (sbp.Price * ri.Count) - ((sbp.Price * ri.Count) * sbp.Off' +
        'er) /100 as sumPrice,'
      '      u.Name AS unitName'
      'FROM RequestItem AS ri'
      'LEFT JOIN StuffBrand AS sb ON (ri.StuffBrandID = sb.ID)'
      'LEFT JOIN Stuff AS s ON (sb.StuffID = s.ID)'
      'LEFT JOIN Brand AS b ON (sb.BrandID = b.ID)'
      
        'LEFT JOIN StuffBrandPrice AS sbp ON (ri.StuffBrandID = sbp.Stuff' +
        'BrandID)'
      'LEFT JOIN Unit AS u ON (sb.UnitID = u.ID)'
      '')
    Left = 32
    Top = 488
