  object qry: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO Request'
      
        '  (RequestDateTime, UserID, StuffCount, SumPrice, Accepted, Acce' +
        'ptedDatetime, Sended, SendedDateTime)'
      'VALUES'
      
        '  (:RequestDateTime, :UserID, :StuffCount, :SumPrice, :Accepted,' +
        ' :AcceptedDatetime, :Sended, :SendedDateTime)'
      'SET :ID = SCOPE_IDENTITY()')
    SQLDelete.Strings = (
      'DELETE FROM Request'
      'WHERE'
      '  ID = :Old_ID')
    SQLUpdate.Strings = (
      'UPDATE Request'
      'SET'
      '  RequestDateTime = :RequestDateTime, '
      '  UserID = :UserID, '
      '  StuffCount = :StuffCount, '
      '  SumPrice = :SumPrice, '
      '  Accepted = :Accepted, '
      '  AcceptedDatetime = :AcceptedDatetime, '
      '  Sended = :Sended, '
      '  SendedDateTime = :SendedDateTime'
      'WHERE'
      '  ID = :Old_ID')
    SQLRefresh.Strings = (
      
        'SELECT RequestDateTime, UserID, StuffCount, SumPrice, Accepted, ' +
        'AcceptedDatetime, Sended, SendedDateTime FROM Request'
      'WHERE'
      '  ID = :ID')
    SQLRecCount.Strings = (
      'SET :PCOUNT = (SELECT COUNT(*) FROM Request'
      ')')
    Connection = dmPakhsh.Con
    SQL.Strings = (
      'SELECT  r.ID,'
      #9#9'r.RequestDateTime,'
      #9#9'r.UserID,'
      #9#9'r.StuffCount,'
      #9#9'r.SumPrice,'
      #9#9'r.Accepted,'
      #9#9'r.AcceptedDatetime,'
      #9#9'r.Sended,'
      #9#9'r.SendedDateTime,'
      #9#9'u.MobileNumber,'
      #9#9'u.FirstName + '#39' '#39' + LastName AS UserName'
      'FROM Request AS r'
      'LEFT JOIN app.Users AS u ON(r.UserID = u.ID)'
      ''
      'ORDER BY r.RequestDateTime')
